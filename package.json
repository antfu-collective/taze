{
  "name": "taze",
  "type": "module",
  "version": "0.8.1",
  "packageManager": "pnpm@7.5.0",
  "description": "A modern cli tool that keeps your deps fresh",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/taze#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/taze.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/taze/issues"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "taze": "bin/taze.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "dev": "cross-env DEBUG=taze:* esmo ./src/cli.ts",
    "start": "esno ./src/cli.ts",
    "build": "unbuild",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --tag --push && pnpm publish --no-git-checks",
    "test": "vitest"
  },
  "dependencies": {
    "@antfu/ni": "^0.18.0",
    "detect-indent": "^7.0.1",
    "libnpmconfig": "^1.2.1",
    "pacote": "^13",
    "prompts": "^2.4.2",
    "semver": "^7.3.7",
    "unconfig": "^0.3.6",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/utils": "^0.5.2",
    "@types/cli-progress": "^3.11.0",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.7.18",
    "@types/pacote": "^11.1.5",
    "@types/prompts": "^2.4.0",
    "@types/semver": "^7.3.12",
    "@types/yargs": "^17.0.12",
    "bumpp": "^8.2.1",
    "cli-progress": "^3.11.2",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "deepmerge": "^4.2.2",
    "eslint": "^8.23.1",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "find-up": "^6.3.0",
    "picocolors": "^1.0.0",
    "rimraf": "^3.0.2",
    "taze": "workspace:*",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vitest": "^0.23.4"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
