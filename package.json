{
  "name": "taze",
  "type": "module",
  "version": "0.13.6",
  "packageManager": "pnpm@9.0.0",
  "description": "A modern cli tool that keeps your deps fresh",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/taze#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/taze.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/taze/issues"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "taze": "bin/taze.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "dev": "cross-env DEBUG=taze:* esno ./src/cli.ts",
    "start": "esno ./src/cli.ts",
    "build": "unbuild",
    "typecheck": "tsc",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish --no-git-checks",
    "test": "unbuild && vitest"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "@jsdevtools/ez-spawn": "^3.0.4",
    "cli-progress": "^3.12.0",
    "deepmerge": "^4.3.1",
    "detect-indent": "^7.0.1",
    "picocolors": "^1.0.0",
    "prompts": "^2.4.2",
    "ufo": "^1.5.3",
    "unconfig": "^0.3.13",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.14.0",
    "@antfu/utils": "^0.7.7",
    "@npmcli/config": "^8.2.2",
    "@types/cli-progress": "^3.11.5",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.12.7",
    "@types/npm-package-arg": "^6.1.4",
    "@types/npm-registry-fetch": "^8.0.7",
    "@types/prompts": "^2.4.9",
    "@types/semver": "^7.5.8",
    "@types/yargs": "^17.0.32",
    "bumpp": "^9.4.0",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "eslint": "^9.0.0",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "npm-package-arg": "^11.0.2",
    "npm-registry-fetch": "^16.2.1",
    "rimraf": "^5.0.5",
    "semver": "^7.6.0",
    "taze": "workspace:*",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vitest": "^1.5.0"
  }
}
