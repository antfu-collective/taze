    "fast-npm-meta": "^0.3.1",
    "npm-package-arg": "^12.0.2",
    "npm-registry-fetch": "^18.0.2",
    "p-limit": "^6.2.0",
    "semver": "^7.7.1",
    "taze": "workspace:*",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "ufo": "^1.5.4",
