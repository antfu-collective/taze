{
  "name": "taze",
  "type": "module",
  "version": "0.8.3",
  "packageManager": "pnpm@7.12.0",
  "description": "A modern cli tool that keeps your deps fresh",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/taze#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/taze.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/taze/issues"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "taze": "bin/taze.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "dev": "cross-env DEBUG=taze:* esno ./src/cli.ts",
    "start": "esno ./src/cli.ts",
    "build": "unbuild",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish --no-git-checks",
    "test": "vitest"
  },
  "dependencies": {
    "@antfu/ni": "^0.18.6",
    "@npmcli/config": "^6.1.0",
    "detect-indent": "^7.0.1",
    "pacote": "^13",
    "prompts": "^2.4.2",
    "semver": "^7.3.8",
    "unconfig": "^0.3.7",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.30.1",
    "@antfu/utils": "^0.6.3",
    "@types/cli-progress": "^3.11.0",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.11.9",
    "@types/pacote": "^11.1.5",
    "@types/prompts": "^2.4.1",
    "@types/semver": "^7.3.13",
    "@types/yargs": "^17.0.13",
    "bumpp": "^8.2.1",
    "cli-progress": "^3.11.2",
    "cross-env": "^7.0.3",
    "debug": "^4.3.4",
    "deepmerge": "^4.2.2",
    "eslint": "^8.27.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "picocolors": "^1.0.0",
    "rimraf": "^3.0.2",
    "taze": "workspace:*",
    "typescript": "^4.9.3",
    "unbuild": "^1.0.1",
    "vitest": "^0.25.2"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
