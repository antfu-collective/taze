    "typecheck": "tsc",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish --no-git-checks",
    "test": "unbuild && vitest"
  },
  "dependencies": {
    "@antfu/ni": "^0.23.0",
    "js-yaml": "^4.1.0",
    "ofetch": "^1.3.4",
